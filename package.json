{
	"name": "quick",
	"displayName": "ke-react-quick-generator",
	"publisher": "weiqiang",
	"description": "generator common code",
	"version": "0.0.2",
	 "repository": {
    "type": "git",
    "url": "hhttps://github.com/mushang11/ke-react"
  },
	"engines": {
		"vscode": "1.40.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
      {
        "language": "javascript",
        "path": "./src/snippets/file.json"
      },
      {
        "language": "typescriptreact",
        "path": "./src/snippets/file.json"
      }
    ],
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
